        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
   if (isLoading || newValue == '') {
      return;
   }

   //Type appropriate comment here, and begin script below
   var dateNeeded = g_form.getValue('date');
   var dateToReturn = g_form.getValue('date_to_return_the_vehicle');

   if (dateToReturn < dateNeeded){
	g_form.addErrorMessage('Date to return the Vehicle should not be before the date the Vehicle is needed');
	g_form.clearValue('date_to_return_the_vehicle');
	return false;
   }
   return true;
   
}]]></script>
